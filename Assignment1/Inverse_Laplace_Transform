%% ASSIGNMENT 1 - INVERSE LAPLACE TRANSFORM
disp("INVERSE LAPLACE TRANSFORM")
syms s

disp("II. Solve for the Inverse Laplace Transform of the following: ");
 
disp("1.) "); 
F3 = (8 - 3*s + s^2) / s^3;
f3 = ilaplace(F3);
pretty(f3)

disp("2.) "); 
F4 = (5 / (s-2)) - (4*s / (s^2 + 9));
f4 = ilaplace(F4);
pretty(f4)

disp("3.) "); 
F5 = (7 / (s^2 + 6));
f5 = ilaplace(F5);
pretty(f5)
